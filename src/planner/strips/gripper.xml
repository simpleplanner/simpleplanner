<strips>
	<domain name="gripper">
		<action name="move">
			<param name="$from" />
			<param name="$to" />
			<precondition>
				<and>
					<predicate name="at-robby">
						<param name="$from" />
					</predicate>
					<predicate name="room">
						<param name="$from" />
					</predicate>
					<predicate name="room">
						<param name="$to" />
					</predicate>
				</and>
			</precondition>
			<effect>
				<and>
					<predicate name="at-robby">
						<param name="$to" />
					</predicate>
					<not>
						<predicate name="at-robby">
							<param name="$from" />
						</predicate>
					</not>
				</and>
			</effect>
		</action>

		<action name="pick">
			<param name="$ball" />
			<param name="$room" />
			<param name="$gripper" />
			<precondition>
				<and>
					<predicate name="ball">
						<param name="$ball" />
					</predicate>
					<predicate name="room">
						<param name="$room" />
					</predicate>
					<predicate name="gripper">
						<param name="$gripper" />
					</predicate>
					<predicate name="at">
						<param name="$ball" />
						<param name="$room" />
					</predicate>
					<predicate name="at-robby">
						<param name="$room" />
					</predicate>
					<predicate name="free">
						<param name="$gripper" />
					</predicate>
				</and>
			</precondition>
			<effect>
				<and>
					<predicate name="carry">
						<param name="$ball" />
						<param name="$gripper" />
					</predicate>
					<not>
						<predicate name="at">
							<param name="$ball" />
							<param name="$room" />
						</predicate>
					</not>
					<not>
						<predicate name="free">
							<param name="$gripper" />
						</predicate>
					</not>
				</and>
			</effect>
		</action>

		<action name="drop">
			<param name="$ball" />
			<param name="$room" />
			<param name="$gripper" ></param>
			<precondition>
				<and>
					<predicate name="ball">
						<param name="$ball" />
					</predicate>
					<predicate name="room">
						<param name="$room" />
					</predicate>
					<predicate name="gripper">
						<param name="$gripper" />
					</predicate>
					<predicate name="carry">
						<param name="$ball" />
						<param name="$gripper" />
					</predicate>
					<predicate name="at-robby">
						<param name="$room" />
					</predicate>
				</and>
			</precondition>
			<effect>
				<and>
					<predicate name="at">
						<param name="$ball" />
						<param name="$room" />
					</predicate>
					<not>
						<predicate name="carry">
							<param name="$ball" />
							<param name="$gripper" />
						</predicate>
					</not>
					<predicate name="free">
						<param name="$gripper" />
					</predicate>
				</and>
			</effect>
		</action>
	</domain>

